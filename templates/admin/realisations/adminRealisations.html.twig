{% extends 'base.html.twig' %}

{% block stylesheets %}{% endblock %}


{% block title %}
	admin.html.twig
{% endblock %}

{% block body %}

	{# {{ dump(realisations)}} #}

	<h1>Gérer les réalisations</h1>

	<table>
		<thead>
			<tr>
				<th>Titre</th>
				<th>Actions</th>
			</tr>
		</thead>
		<tbody>
			{% for realisation in realisations %}
				<tr>
					<td>
						{{ realisation.titre }}
					</td>
					<td>
						<a href="{{ path('admin.realisation.edit', {id: realisation.id}) }}">Editer</a>
					</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>


{% endblock %}
